{
  "manifest_version": 2,
  "name": "WE Find Demo - Basic",
  "description": "WE Find Demo - Basic",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "find",
    "activeTab",
    "tabs"
  ],

  "icons": {
    "32": "icons/button_close_blue_12.png",
    "48": "icons/button_close_blue_12.png"
  },

  "browser_action": {
    "default_icon": "icons/button_close_blue_12.png",
    "default_title": "Find Demo Basic"
  }

}
